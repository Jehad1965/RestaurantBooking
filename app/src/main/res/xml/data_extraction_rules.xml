<?xml version="1.0" encoding="utf-8"?>
<!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Use <include> and <exclude> to control what is backed up. -->
        <include domain="file" path="your_backup_path" />
        <exclude domain="file" path="your_excluded_path" />
    </cloud-backup>
    <device-transfer>
        <!-- Use <include> and <exclude> to control what is transferred. -->
        <include domain="file" path="your_transfer_path" />
        <exclude domain="file" path="your_excluded_transfer_path" />
    </device-transfer>
</data-extraction-rules>
